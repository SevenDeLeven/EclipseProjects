package com.sevendeleven.testproject.item;

public class ItemStack {
	private int item, size, meta;
	public ItemStack(int item, int size, int meta) {
		this.item = item;
		this.size = size;
		this.meta = meta;
	}
	public void setItem(int item) {
		this.item = item;
	}
	public void setSize(int size) {
		this.size = size;
	}
	public void setMeta(int meta) {
		this.meta = meta;
	}
}
